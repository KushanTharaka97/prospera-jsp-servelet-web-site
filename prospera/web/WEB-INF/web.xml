<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>clinicSignup</servlet-name>
        <servlet-class>partners.clinicSignup.clinicSignup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>registerPartners.login.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>acceptPartners</servlet-name>
        <servlet-class>accept.Partners.acceptPartners</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminSignup</servlet-name>
        <servlet-class>admin.signup.adminSignup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>reply</servlet-name>
        <servlet-class>askQuestion.Comment.reply.reply</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deletePartners</servlet-name>
        <servlet-class>partner.deleting.deletePartners</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>asking</servlet-name>
        <servlet-class>askk.commenting.asking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>asking1</servlet-name>
        <servlet-class>askk.commenting.asking1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteAdmin</servlet-name>
        <servlet-class>deleting.deleteAdmin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>clinicSignup</servlet-name>
        <url-pattern>/clinicSignup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>acceptPartners</servlet-name>
        <url-pattern>/acceptPartners</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminSignup</servlet-name>
        <url-pattern>/adminSignup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>reply</servlet-name>
        <url-pattern>/reply</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deletePartners</servlet-name>
        <url-pattern>/deletePartners</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>asking</servlet-name>
        <url-pattern>/asking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>asking1</servlet-name>
        <url-pattern>/asking1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteAdmin</servlet-name>
        <url-pattern>/deleteAdmin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
